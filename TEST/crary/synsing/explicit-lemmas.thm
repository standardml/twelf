	      mwf/sing
	      (Dmaof x d : maof (Daof x d) Rm))
	   (Dlookup : {x} isvar x I -> lookup (G x) x A)
%%
	   ([x] [d'] wfe/sing (Daofe x d'))
	   ([x] [d'] mwfe/sing (Dmaofe x d'))
	   <- cut-aof Rm Daof Dmaof Dlookup
	      (Daofe : {x} isvar x I -> aofe (G x) (R x) t)
