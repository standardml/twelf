	      mwfe/pi
	      (DmwfeC x d d' : {y} {e'} mwfe (DwfeC x d d' y e') Cm)
	      (DmwfeB x d d' : mwfe (DwfeB x d d') Bm))
	   (Dlookup : {x} isvar x I -> lookup (G x) x A)
%%
	   ([x] [d'] wfe/pi (DwfeC' x d') (DwfeB' x d'))
	   ([x] [d'] mwfe/pi (DmwfeC' x d') (DmwfeB' x d'))
