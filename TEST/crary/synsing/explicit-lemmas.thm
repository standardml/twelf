-	: bump2-ofe
	   (Dof1 : {x} isvar x I -> {y} isvar y J -> ofe (G x y) (M x y) (A x y))
	   (Dordered1 : {x} isvar x I -> {y} isvar y J' -> ordered (G x y))
	   (Dordered2 : {x} isvar x I' -> {y} isvar y J' -> ordered (G x y))
	   Dof3
	   <- ({x} {d:isvar x I}
