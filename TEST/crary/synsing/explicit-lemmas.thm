	      (Dsub x : tsub ([y] C x y) (M x) (C' x))
	      (Dofe x d d' : ofe (G x) (M x) (B x))
	      (Daofe x d d' : aofe (G x) (R x) (pi (B x) ([y] C x y))))
	   ([x] [d] [d']
	      maofe/app
	      (Dmwfe x d d' : mwfe (Dwfe x d d') Cm)
	      (Dmofe x d d' : mofe (Dofe x d d') Mm)
